{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "WTM_UAM",
    "description" : "API zum Verwalten User",
    "contact" : {
      "name" : "",
      "email" : ""
    },
    "license" : {
      "name" : "Apache 2.0",
      "url" : "https://www.apache.org/licenses/LICENSE-2.0"
    },
    "version" : "v1.0"
  },
  "servers" : [ {
    "url" : "http://localhost:8080/project",
    "description" : "Entwicklungssystem"
  } ],
  "paths" : {
    "/api/admin/role" : {
      "post" : {
        "operationId" : "createRole",
        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/Role"
              }
            }
          },
          "required" : true
        },
        "responses" : {
          "200" : {
            "content" : {
              "*/*" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Role"
                }
              }
            },
            "description" : "OK"
          }
        },
        "tags" : [ "role-controller" ]
      }
    },
    "/api/admin/role/all" : {
      "get" : {
        "operationId" : "getAllRoles",
        "responses" : {
          "200" : {
            "content" : {
              "*/*" : {
                "schema" : {
                  "type" : "array",
                  "items" : {
                    "$ref" : "#/components/schemas/Role"
                  }
                }
              }
            },
            "description" : "OK"
          }
        },
        "tags" : [ "role-controller" ]
      }
    },
    "/api/user/all" : {
      "get" : {
        "operationId" : "allProjects",
        "responses" : {
          "200" : {
            "content" : {
              "*/*" : {
                "schema" : {
                  "type" : "array",
                  "items" : {
                    "$ref" : "#/components/schemas/User"
                  }
                }
              }
            },
            "description" : "OK"
          }
        },
        "tags" : [ "user-controller" ]
      }
    },
    "/api/user/delete/{id}" : {
      "delete" : {
        "operationId" : "deleteUser",
        "parameters" : [ {
          "in" : "path",
          "name" : "id",
          "required" : true,
          "schema" : {
            "type" : "string"
          }
        } ],
        "responses" : {
          "200" : {
            "content" : {
              "*/*" : {
                "schema" : {
                  "type" : "boolean"
                }
              }
            },
            "description" : "OK"
          }
        },
        "tags" : [ "user-controller" ]
      }
    },
    "/api/user/save" : {
      "post" : {
        "operationId" : "saveUser",
        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/User"
              }
            }
          },
          "required" : true
        },
        "responses" : {
          "200" : {
            "content" : {
              "*/*" : {
                "schema" : {
                  "$ref" : "#/components/schemas/User"
                }
              }
            },
            "description" : "OK"
          }
        },
        "tags" : [ "user-controller" ]
      }
    },
    "/api/user/{id}" : {
      "get" : {
        "operationId" : "getUser",
        "parameters" : [ {
          "in" : "path",
          "name" : "id",
          "required" : true,
          "schema" : {
            "type" : "string"
          }
        } ],
        "responses" : {
          "200" : {
            "content" : {
              "*/*" : {
                "schema" : {
                  "$ref" : "#/components/schemas/User"
                }
              }
            },
            "description" : "OK"
          }
        },
        "tags" : [ "user-controller" ]
      }
    }
  },
  "components" : {
    "schemas" : {
      "Role" : {
        "type" : "object",
        "properties" : {
          "description" : {
            "type" : "string"
          },
          "id" : {
            "type" : "string"
          },
          "name" : {
            "type" : "string"
          },
          "permissionsLevel" : {
            "type" : "integer",
            "format" : "int32"
          }
        }
      },
      "User" : {
        "type" : "object",
        "properties" : {
          "email" : {
            "type" : "string"
          },
          "id" : {
            "type" : "string"
          },
          "password" : {
            "type" : "string"
          },
          "role" : {
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/Role"
            }
          },
          "username" : {
            "type" : "string"
          }
        }
      }
    }
  }
}