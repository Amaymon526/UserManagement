{
  "openapi": "3.0.1",
  "info": {
    "title": "JEFF'S REST-API",
    "description": "API zum Verwalten von Listen",
    "contact": {
      "name": "Jeffrey Nelwek",
      "email": "jeff.nelwek@gmail.com"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "https://www.apache.org/licenses/LICENSE-2.0"
    },
    "version": "v3.0"
  },
  "servers": [
    {
      "url": "http://localhost:8080/project",
      "description": "Entwicklungssystem"
    }
  ],
  "paths": {
    "/api/user/all": {
      "get": {
        "operationId": "allProjects",
        "responses": {
          "200": {
            "content": {
              "*/*": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/User"
                  }
                }
              }
            },
            "description": "OK"
          }
        },
        "tags": [
          "user-controller"
        ]
      }
    },
    "/api/user/delete/{id}": {
      "delete": {
        "operationId": "deleteUser",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "*/*": {
                "schema": {
                  "type": "boolean"
                }
              }
            },
            "description": "OK"
          }
        },
        "tags": [
          "user-controller"
        ]
      }
    },
    "/api/user/save": {
      "post": {
        "operationId": "saveUser",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/User"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/User"
                }
              }
            },
            "description": "OK"
          }
        },
        "tags": [
          "user-controller"
        ]
      }
    },
    "/api/user/user/{id}": {
      "get": {
        "operationId": "getUser",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/User"
                }
              }
            },
            "description": "OK"
          }
        },
        "tags": [
          "user-controller"
        ]
      }
    }
  },
  "components": {
    "schemas": {
      "User": {
        "type": "object",
        "properties": {
          "city": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "houseNumber": {
            "type": "string"
          },
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "phone": {
            "type": "string"
          },
          "street": {
            "type": "string"
          },
          "username": {
            "type": "string"
          },
          "zipCode": {
            "type": "string"
          }
        }
      }
    }
  }
}